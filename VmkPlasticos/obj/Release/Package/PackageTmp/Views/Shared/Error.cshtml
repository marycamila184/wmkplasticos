    <script src="http://maps.googleapis.com/maps/api/js?key=AIzaSyD4pZSF5XJf_UAWX6sszRPgkBVTIA5SqnY&sensor=false" type="text/javascript"></script>
    <script src="~/Content/js/jquery.gmap.min.js"></script>
    <script>
        // MAP
        jQuery(document).ready(function ($) {
            $("#responsive_map").gMap({
                maptype: google.maps.MapTypeId.ROADMAP,
                zoom: 16,
                markers: [{
                    latitude: -25.493756,
                    longitude: -49.2380935,
                    html: "<strong>Estamos localizados aqui!<br> Rua Bartolomeu Lourenço de Gusmão, 2480 </strong><br/> <br> Hauer, Curitiba - PR, 80050-540 <br/>",
                    popup: true,
                    flat: true,
                    icon: {
                        iconsize: [32, 37],
                        shadow: "Content/images/icon-shadow.png",
                        iconanchor: [15, 30],
                        shadowsize: [32, 37],
                        shadowanchor: null
                    }
                }
                ],
                panControl: true,
                zoomControl: true,
                mapTypeControl: true,
                scaleControl: true,
                streetViewControl: true,
                scrollwheel: false,
                styles: [{ "stylers": [{ "hue": "#080e73" }, { "gamma": 0 }] }],
                onComplete: function () {
                    // Resize and re-center the map on window resize event
                    var gmap = $("#responsive_map").data('gmap').gmap;
                    window.onresize = function () {
                        google.maps.event.trigger(gmap, 'resize');
                        $("#responsive_map").gMap('fixAfterResize');
                    };
                }
            });
        });
    </script>
