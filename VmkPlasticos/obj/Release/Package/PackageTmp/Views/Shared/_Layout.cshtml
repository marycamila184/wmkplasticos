        function makeListProducs() {
            $("#lisProducts").html = '';
            $("tr").each(function () {
                $this = $(this);
                var nome = $this.find("td span.nome").text();
                var quantidade = $this.find("td span.quantidade").text();
                var unidade = $this.find("td span.unidade").text();

                var tag = "<input id='Products_" + this.id + "__NomeProduto' name='Products[" + this.id + "].NomeProduto' type='text' value='" + nome + "'>";
                tag += "<input id='Products_" + this.id + "__Quantidade' name='Products[" + this.id + "].Quantidade' type='text' value='" + quantidade + "'>";
                tag += "<input id='Products_" + this.id + "__Unidades' name='Products[" + this.id + "].Unidades' type='text' value='" + unidade + "'>";
                $("#lisProducts").append(tag);
            });

            $("#form-budget").submit();
        }

        function addRow() {
            var nomeProduto = '';
            if ($('#linha-ka').css('display') == 'none') {
                nomeProduto = $("#linha-vmk :selected").text();
            } else {
                nomeProduto = $("#linha-ka :selected").text();
            }
            var quantidade = $('#quantity').val();
            var unidade = $("#unid-min-label").text();

            var table = $('#table-produtos tr').length;
            var table_len = table - 1;

            var row = $('#table-produtos tbody').append("<tr id='" + table_len + "'><td><span class='nome'>" + nomeProduto + "</span></td><td><span class='quantidade'>" + quantidade + "</span> <span class='unidade'>" + unidade + "</span></td><td><a class='btn btn-danger' onclick='return deleteRow(" + table_len + ");'><span class='glyphicon glyphicon-trash'></span>   Remover</td></tr>");
        }

        function deleteRow(id) {
            $("#" + (id)).html('');
        }

        $(document).ready(function () {
            var quantitiy = 0;
            $('.quantity-right-plus').click(function (e) {
                e.preventDefault();
                var quantity = parseInt($('#quantity').val());
                $('#quantity').val(quantity + 1);
            });

            $('.quantity-left-minus').click(function (e) {
                e.preventDefault();
                var quantity = parseInt($('#quantity').val());

                var linha = $("#linha").val();
                if (linha == 'ka') {
                    if (quantity > 250) {
                        $('#quantity').val(quantity - 1);
                    }
                }
                else {
                    if (quantity > 0) {
                        $('#quantity').val(quantity - 1);
                    }
                }
            });

        });
    </script>
